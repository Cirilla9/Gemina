+        vcpu_count = atomic_read(&(vcpu->kvm->online_vcpus));
+        if (vcpu_id < vcpu_count) {
+                vcpu = kvm_get_vcpu(vcpu->kvm, vcpu_id);
+                if (!vcpu)
